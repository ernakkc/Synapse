{
  "name": "synapse",
  "version": "1.0.0",
  "description": "> Synapse is an AI-powered assistant that provides intelligent responses to user queries. Cross-platform AI Assistant with centralized brain architecture built on Electron, React & TypeScript.",
  "keywords": [
    "ai",
    "assistant",
    "clean-architecture",
    "enterprise",
    "electron",
    "telegram",
    "typescript",
    "modular",
    "pluggable",
    "openai",
    "anthropic",
    "ollama",
    "cross-platform",
    "event-driven",
    "ddd",
    "solid"
  ],
  "homepage": "https://github.com/ernakkc/Synapse#readme",
  "bugs": {
    "url": "https://github.com/ernakkc/Synapse/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ernakkc/Synapse.git"
  },
  "private": false,
  "license": "MIT",
  "author": {
    "name": "Eren Akkoc",
    "email": "ern.akkc@gmail.com",
    "url": "https://erenakkoc.com"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "type": "module",
  "main": "dist/main.js",
  "scripts": {
    "------------------ RUNNING ------------------": "",
    "start": "tsx src/main.ts",
    "cli": "tsx src/interfaces/cli/index.ts",
    "------------------ BUILDING -----------------": "",
    "build": "tsc",
    "----------------- DEVELOPMENT ---------------": "",
    "dev": "tsx watch src/main.ts",
    "------------------ TESTING ------------------": "",
    "test": "vitest run",
    "test:unit": "vitest run tests/unit",
    "test:integration": "vitest run tests/integration",
    "test:watch": "vitest watch",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "test:config": "tsx config/config.ts",
    "test:logger": "tsx tests/logger.test.ts"
  },
  "dependencies": {
    "dotenv": "^17.2.4",
    "ollama": "^0.6.3"
  },
  "devDependencies": {
    "@types/node": "^25.2.2",
    "@vitest/coverage-v8": "^4.0.18",
    "tsx": "^4.21.0",
    "winston": "^3.19.0"
  },
  "optionalDependencies": {
    "@rollup/rollup-darwin-x64": "^4.57.1"
  }
}
